{
  "name": "@crbroughton/nuxt-cache",
  "version": "0.1.1",
  "description": "A Nuxt Cache Module",
  "author": "Craig R Broughton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crbroughton/nuxt-cache.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && npm publish  --access=public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "changeset": "npx @changesets/cli",
    "changeset:status": "npx @changesets/cli status --verbose",
    "changeset:version": "npx @changesets/cli version"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/eslint-config": "^1.0.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.20.1",
    "nuxt": "^3.15.4",
    "typescript": "~5.7.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  }
}
